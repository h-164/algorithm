function solution(d, budget) {
    let answer = 0;
    
    d = d.sort(function(a,b){
        return a - b
    })
    
    let plus = d.reduce((nj,hj,i,arr)=>{
        nj + hj > budget? answer -= 1 && arr.splice(1) : answer = answer
        answer += 1
        return nj + hj;
    },0)
    
    return answer
}
