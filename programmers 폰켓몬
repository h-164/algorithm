function solution(nums) {
    var answer = 0;
    
    nums.sort(function(a, b) {
        return a - b;
    });
    
    let arr =[];
    for(let i = 0; i < nums.length; i++){
        if(arr[arr.length-1] !== nums[i]){
            arr.push(nums[i])
        }
        
        if(arr.length === nums.length/2){
            break
        }
    }
    
    return arr.length;
}
