class Stack {
  constructor() {
    this.arr = [];
    this.index = -1;
    this.top = 0;
    this.length = 0;
  }
  push(item) {
    this.arr.push(item);
    this.index++;
    this.top = item;
    this.length++;
  }
  pop() {
    if (this.length === 0) return null;
    this.index--;
    this.top = this.arr[this.index];
    this.length--;
    return this.arr.pop(this.length);
  }
}

let stack = new Stack();
